cd /go/src/github.com/raintank/raintank-metric/metric_tank
/wait.sh cassandra:9042
./metric_tank \
	--chunkspan 600 \
	--numchunks 3 \
	--agg-settings 60:120:2,300:1800:2,400:600:1 \
	--statsd-addr statsdaemon:8125 \
	--nsqd-tcp-address nsqd:4150 \
	--listen :6063 \
	--cassandra-addrs cassandra &>> /var/log/raintank/nsq_metrics_tank.log &
tail -f /var/log/raintank/nsq_metrics_tank.log 
